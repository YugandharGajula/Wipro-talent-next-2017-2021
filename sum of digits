import java.util.*;
public class SumOfDigits
{
	public static void main(String[] args) {
	 System.out.println("Enter a number");
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		int i,sum=0,rem;
		for(i=0;num!=0;i++)
		{
		    rem=num%10;
		    sum=sum+rem;
		    num=num/10;
		}
		System.out.println(sum);
	}
}

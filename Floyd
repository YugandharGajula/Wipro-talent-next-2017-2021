import java.util.*;
public class Floyd
{
	public static void main(String[] args) {
		System.out.println("Enter the number of rows:");
		Scanner sc=new Scanner(System.in);
		int rows=sc.nextInt();
		int i,j;
		for(i=1;i<=rows;i++)
		{
		    for(j=1;j<=i;j++)
		    {
		        System.out.print("*");
		    }
		    System.out.println();
		}
	}
}
